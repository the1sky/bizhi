.newsbox.panel.panel-default
	.panel-heading
		span.glyphicon.glyphicon-list-alt
		b 联系我们
	.panel-body
		.row
			.col-xs-12
				if contactme
					ul.contact-me-ul
						li 联系人:
							span	#{contactme.name}
						li 电话：
							span	#{contactme.phone}
						li 传真：
							span	#{contactme.fax}
						li 地址：
							span	#{contactme.address}
	.panel-footer  

